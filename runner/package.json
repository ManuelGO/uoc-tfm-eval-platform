{
  "name": "runner",
  "version": "1.0.0",
  "type": "module",
  "description": "UOC TFM Eval Platform - Runner Service",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest"
  },
  "keywords": [
    "runner",
    "evaluation",
    "submission",
    "sqs"
  ],
  "author": "Manuel de Jesús González Arvelo",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/client-sqs": "^3.935.0",
    "dotenv": "^16.4.5",
    "extract-zip": "^2.0.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/extract-zip": "^2.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.10",
    "eslint": "^9.18.0",
    "jest": "^30.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.48.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ]
  }
}

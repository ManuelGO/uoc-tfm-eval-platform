# AWS Configuration
AWS_REGION=eu-south-2
AWS_S3_BUCKET=uoc-tfm-eval-platform
AWS_SQS_QUEUE_URL=https://sqs.eu-south-2.amazonaws.com/<account-id>/submissions-queue

# Database Configuration
DATABASE_URL=postgresql://user:password@host:5432/dbname
# Or individual variables:
# DB_HOST=localhost
# DB_PORT=5432
# DB_USERNAME=postgres
# DB_PASSWORD=password
# DB_NAME=tfmdb

# Runner Configuration
RUNNER_POLL_INTERVAL_MS=20000
RUNNER_TIMEOUT_MS=120000
RUNNER_MAX_LOG_BYTES=200000

# Build Tools
JDK_VERSION=17
BUILD_TOOL=maven

# ECR URIs (for reference)
ECR_API_URI=451747690955.dkr.ecr.eu-south-2.amazonaws.com/uoc-tfm/api
ECR_RUNNER_URI=451747690955.dkr.ecr.eu-south-2.amazonaws.com/uoc-tfm/runner